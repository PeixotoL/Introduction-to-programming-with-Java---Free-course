package com.TheMasterMindGroup.Loops;

public class ForLoops {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int sum = 0;
		for(int i = 0; i < 10; i++) {
			
			
			
			System.out.println("The value of i is: " + i);
			sum += i;
		}
		System.out.println(sum);
		
		
	}

}
